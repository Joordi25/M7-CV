@{
    ViewData["Title"] = "MisConocimientos";
}



<h1>@ViewData["Title"]</h1>

<p>Esto es Mis Conocimientos.</p>

<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Lenguaje</th>
      <th scope="col">Nivel</th>
      <th scope="col">Experiencia (años)</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>PHP</td>
      <td><div class="progress"><div class="progress-bar" role="progressbar" aria-valuenow="92" aria-valuemin="0" aria-valuemax="100" style="width: 85%;"></div> </div></td>
      <td>1</td>
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>JavasCript</td>
      <td><div class="progress"><div class="progress-bar bg-warning" role="progressbar" aria-valuenow="87" aria-valuemin="0" aria-valuemax="100" style="width: 70%;"></div> </div></td>
      <td>1</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td>Python</td>
      <td><div class="progress"><div class="progress-bar bg-success" role="progressbar" aria-valuenow="87" aria-valuemin="0" aria-valuemax="100" style="width: 90%;"></div></div></td>
      <td>2</td>
    </tr>
    <tr>
      <th scope="row">4</th>
      <td>Java</td>
      <td><div class="progress"><div class="progress-bar bg-danger" role="progressbar"  aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width: 60%;"></div> </div></td>
      <td>3</td>
    </tr>
    <tr>
      <th scope="row">5</th>
      <td>HTML/CSS</td>
      <td><div class="progress"><div class="progress-bar bg-warning" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 90%;"></div> </div></td>
      <td>3</td>
    </tr>
    <tr>
      <th scope="row">6</th>
      <td>ASP.NET</td>
      <td><div class="progress"><div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 70%;"></div> </div></td>
      <td>1</td>
    </tr>
  </tbody>
</table>

<br /> <br />

<img class="lenguajes" src="~/images/lenguajes.jpg" />
